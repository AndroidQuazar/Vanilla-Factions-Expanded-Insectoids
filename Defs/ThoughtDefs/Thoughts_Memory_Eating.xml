<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <ThoughtDef>
    <defName>VFEI_AteRoyalMeal</defName>
    <durationDays>1</durationDays>
    <stages>
      <li>
        <label>ate royal meal</label>
        <description>I ate an extravagant meal. Yum!</description>
        <baseMoodEffect>8</baseMoodEffect>
      </li>
    </stages>
  </ThoughtDef>

  <ThoughtDef>
    <defName>VFEI_RoyalJellyWithdrawal</defName>
    <workerClass>ThoughtWorker_Hediff</workerClass>
    <hediff>VFEI_RoyalJellyAddiction</hediff>
    <validWhileDespawned>true</validWhileDespawned>
    <stages>
      <li>
        <visible>false</visible>
      </li>
      <li>
        <label>kaleidoscope withdrawal</label>
        <description>I feel like the world isn't given me the right angle to see things clearly. I never thought I'd want that juicy jelly of an insectoid so much.</description>
        <baseMoodEffect>-10</baseMoodEffect>
      </li>
    </stages>
  </ThoughtDef>

</Defs>