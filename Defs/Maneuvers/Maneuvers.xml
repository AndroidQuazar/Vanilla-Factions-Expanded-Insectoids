<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Queen -->
    <ManeuverDef>
        <defName>VFEI_EchoingVoice</defName>
        <requiredCapacity>VFEI_EchoingVoice</requiredCapacity>
        <verb>
            <verbClass>Verb_MeleeAttackDamage</verbClass>
            <meleeDamageDef>VFEI_EchoingVoice</meleeDamageDef> <!-- Stun -->
        </verb>
        <logEntryDef>MeleeAttack</logEntryDef>
        <combatLogRulesHit>Maneuver_Slash_MeleeHit</combatLogRulesHit>
        <combatLogRulesDeflect>Maneuver_Slash_MeleeDeflect</combatLogRulesDeflect>
        <combatLogRulesMiss>Maneuver_Slash_MeleeMiss</combatLogRulesMiss>
        <combatLogRulesDodge>Maneuver_Slash_MeleeDodge</combatLogRulesDodge>
    </ManeuverDef>

    <!-- Empress -->
    <ManeuverDef>
        <defName>VFEI_Shrill</defName>
        <requiredCapacity>VFEI_Shrill</requiredCapacity>
        <verb>
            <verbClass>Verb_MeleeAttackDamage</verbClass>
            <meleeDamageDef>VFEI_Shrill</meleeDamageDef> <!-- Confusion -->
        </verb>
        <logEntryDef>MeleeAttack</logEntryDef>
        <combatLogRulesHit>Maneuver_Slash_MeleeHit</combatLogRulesHit>
        <combatLogRulesDeflect>Maneuver_Slash_MeleeDeflect</combatLogRulesDeflect>
        <combatLogRulesMiss>Maneuver_Slash_MeleeMiss</combatLogRulesMiss>
        <combatLogRulesDodge>Maneuver_Slash_MeleeDodge</combatLogRulesDodge>
    </ManeuverDef>

    <ManeuverDef>
        <defName>VFEI_Leap</defName>
        <requiredCapacity>VFEI_Leap</requiredCapacity>
        <verb>
            <verbClass>Verb_MeleeAttackDamage</verbClass>
            <meleeDamageDef>VFEI_Leap</meleeDamageDef> <!-- Jump Attack -->
        </verb>
        <logEntryDef>MeleeAttack</logEntryDef>
        <combatLogRulesHit>Maneuver_Slash_MeleeHit</combatLogRulesHit>
        <combatLogRulesDeflect>Maneuver_Slash_MeleeDeflect</combatLogRulesDeflect>
        <combatLogRulesMiss>Maneuver_Slash_MeleeMiss</combatLogRulesMiss>
        <combatLogRulesDodge>Maneuver_Slash_MeleeDodge</combatLogRulesDodge>
    </ManeuverDef>

</Defs>