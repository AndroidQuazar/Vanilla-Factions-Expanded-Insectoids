<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- Archotech Psychic Nova -->
  <DamageDef ParentName="StunBase">
	<defName>VFEI_PsychicNova</defName>
	<label>psychic nova</label>
	<!-- <harmsHealth>false</harmsHealth>
	<makesBlood>false</makesBlood> -->
	<!-- hediff -->
	<externalViolence>false</externalViolence>
	<deathMessage>{0} has given in to psychic pain.</deathMessage>
	<impactSoundType>Electric</impactSoundType>
	<defaultDamage>20</defaultDamage>
	<explosionSnowMeltAmount>5</explosionSnowMeltAmount>
	<explosionCellMote>Mote_BlastEMP</explosionCellMote> <!-- <explosionCellMote>Mote_BlastDry</explosionCellMote> Can only choose one-->
	<explosionColorEdge>(0.8, 0.4, 0.2, 0.6)</explosionColorEdge>
	<explosionInteriorMote>Mote_ElectricalSpark</explosionInteriorMote>
	<soundExplosion>Explosion_Stun</soundExplosion>
	<combatLogRules>Damage_Stun</combatLogRules>
  </DamageDef>

  <DamageDef>
    <defName>VFEI_PsychicNoDamage</defName>
    <label>psychic</label>
    <canInterruptJobs>false</canInterruptJobs>
    <makesBlood>false</makesBlood>
    <defaultDamage>0</defaultDamage>
    <explosionCellMote>Mote_BlastExtinguisher</explosionCellMote>
    <explosionColorEdge>(1, 1, 1, 0.05)</explosionColorEdge>
    <harmsHealth>false</harmsHealth>
    <soundExplosion>Explosion_Smoke</soundExplosion>
    <combatLogRules>Damage_Smoke</combatLogRules>
  </DamageDef>

</Defs>